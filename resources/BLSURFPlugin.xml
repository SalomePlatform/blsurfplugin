<?xml version='1.0' encoding='us-ascii'?>
<!DOCTYPE meshers PUBLIC "" "desktop.dtd">

<!--  GUI customization for BLSURF Plugin  -->

<meshers>

<meshers-group name="BLSURF"
               resources="BLSURFPlugin"
               server-lib="BLSURFEngine"
               gui-lib="BLSURFPluginGUI">
